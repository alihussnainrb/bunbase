{
	"name": "@bunbase/react",
	"version": "1.0.1",
	"description": "Fully-typed React client for Bunbase with TanStack Query integration",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "bunup",
		"dev": "bunup --watch",
		"type-check": "tsc --noEmit",
		"release": "bumpp -r --commit --push --tag"
	},
	"keywords": [
		"bunbase",
		"react",
		"typescript",
		"api-client",
		"tanstack-query",
		"react-query"
	],
	"author": "Bunbase",
	"license": "MIT",
	"peerDependencies": {
		"@tanstack/react-query": "^5.0.0",
		"react": "^18.0.0"
	},
	"devDependencies": {
		"@tanstack/react-query": "^5.62.11",
		"@types/react": "^18.3.18",
		"react": "^18.3.1",
		"typescript": "^5.7.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bunbase/bunbase.git",
		"directory": "packages/react"
	}
}
